    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

    <link rel="stylesheet" href="/css/style.css" />
    <title><%=pageName%></title>

    <style>
      @media (max-width: 990px) {
        .navbar-collapse {
          text-align: center !important;
        }
        #carouselExampleControls {
          margin-top: 0rem !important;
        }
        .btn {
          margin-top: 0.5rem;
        }
        #navbar {
          background-color: rgb(183, 188, 192);
        }
        .nav2 {
          margin-left: -2rem !important;
        }
      }
      nav {
        z-index: +2;
      }
      .navbar-brand {
        font-family: "Black Ops One", sans-serif;
        font-size: x-large;
      }
      .btn-outline-dark:hover {
        color: aliceblue !important;
      }
      .navbar-nav {
        font-family: "Montserrat 600", sans-serif;

        font-size: large;
      }
      .dropdown-item:hover {
        color: black !important;
      }
      #navbar {
        min-height: 100vh;
      }
      #page1 {
        background-color: aliceblue;
      }
      #carouselExampleControls img {
        max-height: 100vh;
        min-height: 100vh;
        object-fit: cover;
      }
      #carouselExampleControls {
        z-index: 1;
        margin-top: -4rem;
      }
      .grey-bg {
        background-color: #f5f7fa;
      }

      #nav .card:hover {
        background-color: rgb(19, 18, 18);
        .title {
          color: rgba(233, 78, 94, 0.747);
        }
      }
      @media (max-width: 767.98px) {
        .border-sm-start-none {
          border-left: none !important;
        }
      }
      .loader {
  width: 48px;
  height: 48px;
  border-radius: 50%;
  display: inline-block;
  position: relative;
  background: linear-gradient(0deg, rgba(255, 61, 0, 0.2) 33%, #ff3d00 100%);
  box-sizing: border-box;
  animation: rotation 1s linear infinite;
}
.loader::after {
  content: '';  
  box-sizing: border-box;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 44px;
  height: 44px;
  border-radius: 50%;
  background: #ffffff;
}
@keyframes rotation {
  0% { transform: rotate(0deg) }
  100% { transform: rotate(360deg)}
} 
    
  #content{
    display: none;
  }
    </style>
  </head>

  <body>
    <section id="loader" style="justify-content: center;text-align: center;min-height: 100vh;min-width: 100%;align-items: center;display: flex;">
      <span class="loader"></span>
    </section>
    <section id="content">
